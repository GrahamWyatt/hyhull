<% if target.errors.any? %>
<div class="flash-message-container alert-error" id="errorExplanation">
  <a href="#" class="close" data-dismiss="alert">&times;</a>
  <strong><%= pluralize(target.errors.count, "error") %> prevented this record from being saved:</strong>
  <ul>
  <% target.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
  <% end %>
  </ul>
</div>
<% end %>


